<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="fillBg" format="reference"></attr>

    <declare-styleable name="trangle_style">
        <attr name="fillBg" />
    </declare-styleable>

    
    <attr name="grid_border"  format="reference" />
    <attr name="is_draw_border" format="boolean"/>
    <attr name="is_adjust_scroll_parent" format="boolean" />
    <declare-styleable name="menu_grid">
        <attr name="is_draw_border"/>
        <attr name="grid_border"/>
        <attr name="is_adjust_scroll_parent"/>
    </declare-styleable>
    
    <attr name="columns" format="integer"/>
    <attr name="cell_padding" format="dimension"></attr>
    <declare-styleable name="line_layout_style">
    	<attr name="columns"></attr>
    	<attr name="cell_padding"></attr>
    </declare-styleable>
</resources>