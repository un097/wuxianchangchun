<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FlowIndicator">
        <attr name="count" format="integer" />
        <attr name="space" format="dimension" />
        <attr name="point_size" format="dimension" />
        <attr name="point_seleted_color" format="color|reference" />
        <attr name="point_normal_color" format="color|reference" />
        <attr name="point_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="w_border_width" format="dimension" />
        <attr name="w_border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="IconButton">
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="between" format="dimension"/>
        <attr name="button_icon" format="reference"/>
        <attr name="textColor" format="color|reference"/>
    </declare-styleable>
</resources>